    "from dolfin import *\n",
    "\n",
    "mesh = UnitSquareMesh(4, 4)\n",
    "\n",
    "ff = FacetFunction('size_t', mesh, 0)\n",
    "for f in facets(mesh):\n",
    "  ff[f] = f.index()"
